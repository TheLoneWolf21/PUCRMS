<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_alignParentEnd="true"
    android:layout_alignParentStart="true"
    android:background="@drawable/poplayouttrasparancy"
    android:layout_alignParentTop="true"
    android:layout_gravity="top">
    <RelativeLayout
        android:id="@+id/cross"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/popsecondlayout"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp">
        <ImageButton
            android:id="@+id/cancel_icon"
            android:layout_width="18dp"
            android:layout_height="match_parent"
            android:src="@drawable/cancel60"
            android:clickable="true"
            android:background="@drawable/cancelripple"
            android:focusable="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/thirdRL"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/cross">
        <TextView
            android:id="@+id/cntreline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_centerVertical="true"/>
        <RelativeLayout
            android:id="@+id/alarm_click"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@drawable/cancelripple"

            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:clickable="true"
            android:focusable="true"
            >
            <ImageView
                android:id="@+id/alarm_icon"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:src="@drawable/alarmclock60"
                android:layout_alignParentBottom="true"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/alarm_icon"
                android:layout_alignTop="@id/alarm_icon"
                android:layout_alignBottom="@id/alarm_icon"
                android:text="Alarm (Feature Coming Soon)"
                android:textColor="#000"
                android:gravity="center|left"
                android:layout_marginStart="12dp"
                android:layout_centerVertical="true"
                android:background="@drawable/popsecondlayout"
                />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/Overlap_click"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@id/alarm_click"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:layout_marginTop="8dp"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/cancelripple"
            >
            <ImageView
                android:id="@+id/overlap_icon"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:src="@drawable/overlap"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/overlap_icon"
                android:layout_alignTop="@id/overlap_icon"
                android:layout_alignBottom="@id/overlap_icon"
                android:text="Overlap"
                android:textColor="#000"
                android:gravity="center|left"
                android:layout_marginStart="12dp"
                android:layout_centerVertical="true"
                android:background="@drawable/popsecondlayout"/>

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/feedbackclick"
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_below="@id/Overlap_click"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:clickable="true"
            android:focusable="true"
            android:background="@drawable/cancelripple"
            >
            <ImageView
                android:id="@+id/feedback_icon"
                android:layout_width="26dp"
                android:layout_height="26dp"
                android:src="@drawable/feedback60"
                android:layout_centerVertical="true"/>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_toEndOf="@id/feedback_icon"
                android:layout_alignTop="@id/feedback_icon"
                android:layout_alignBottom="@id/feedback_icon"
                android:text="Feedback"
                android:textColor="#000"
                android:gravity="center|left"
                android:layout_marginStart="12dp"
                android:layout_centerVertical="true"
                android:background="@drawable/popsecondlayout"
                />

        </RelativeLayout>

    </RelativeLayout>






</RelativeLayout>